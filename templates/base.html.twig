<!DOCTYPE html>
<html lang="de">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <title>{{ app.request.server.get('APP_TITLE') }} - {% block title %}{% endblock %}</title>
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}
        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body class="bg-dark">
    <div id="binary"></div>
        {% block body %}{% endblock %}
        <footer class="fixed-bottom">
            {% block footer %}{% endblock %}
        </footer>
    <a href="{{ path('app_management') }}" ><div class="powered-by"></div></a>
    </body>
</html>
